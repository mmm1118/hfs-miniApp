
@font-face {
    font-family: 'FZYaoTi-M06';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('FZYTK--GBK1-0.ttf') format('truetype'),
        url('FZYTK--GBK1-0.svg#FZYTK--GBK1-0') format('svg');
    font-weight: normal;
    font-style: normal;
}

view{
  font-family:'FZYaoTi-M06';
}

page{
  height: 100%;
  background-color: #eeeef3;
  font-size: 28rpx;
  position: relative;
  -webkit-font-smoothing: antialiased;
}


.center{text-align:center;}

.fx_active{background:red;color:#fff;}

.pr{position:relative}
.po{position:absolute}

.flex{display: flex;}
.flex_between{display:flex;justify-content:space-between;}
.flex_around{display:flex;justify-content: space-around;}
.flex_end{display:flex;justify-content: flex-end;}
.flex_center{display:flex;justify-content: center;}
.flex_Middle{display:flex;align-items:center;justify-content:center;}
.flex_wrap{display:flex;flex-wrap:wrap;}
.flex_hcenter{display: flex; align-items:center;}

.ellipse{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  }

/* 发布按钮 */
.releaseView{
  position: fixed;
  right: 10rpx;
  bottom: 70rpx;
}

.releaseImg{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}

.moduleView{
  border-radius: 20rpx;
  background-color: #fff;
  margin-top: 20rpx;
  padding: 16rpx 26rpx 22rpx 26rpx;
}

.up{
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 22rpx;
  font-weight: bold;
  margin-bottom: 18rpx;
}

.flag{
  width: 4rpx;
  height: 28rpx;
  background-color: #000;
  margin-right: 6rpx;
}

.signIn{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
}

.signInContent{
  display: flex;
  flex-direction: column;
}

.signInContentView{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  background-color: #D61800;
  font-size: 18rpx;
  color: #fff;
  display: flex;
  align-items: center;
  white-space: pre-wrap;
  justify-content: center;
}

.signInContentViewbg{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  background-color: #eeeef3;
  font-size: 18rpx;
  color: #000;
  display: flex;
  align-items: center;
  white-space: pre-wrap;
  justify-content: center;
  text-align: center;
}


.notSignIn{
  background-color: #EDEDF2;
}

.signInContentDate{
  width: 70rpx;
  margin-top: 6rpx;
  text-align: center;
  color: #939393;
  font-size: 16rpx;
}

.signInContentDate{
  width: 70rpx;
  margin-top: 6rpx;
  text-align: center;
  color: #939393;
  font-size: 16rpx;
}

.signInContentDatebg{
  width: 70rpx;
  margin-top: 6rpx;
  text-align: center;
  color: #000;
  font-size: 16rpx;
}

